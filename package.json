{
  "name": "vue-circle-flags",
  "private": false,
  "version": "1.0.8",
  "description": "A vue component with a collection of 400+ circular SVG country flags. Wrapper of HatScripts/circle-flags",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./vue-circle-flags.cjs.js",
  "module": "./dist/vue-circle-flags.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-circle-flags.es.js",
      "require": "./dist/vue-circle-flags.cjs.js"
    },
    "./dist/vue-circle-flags.css": {
      "import": "./dist/vue-circle-flags.css",
      "require": "./dist/vue-circle-flags.css"
    }
  },
  "types": "./dist/components/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test-single": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.ts --ignore-path .eslintignore .",
    "prettier-format": "prettier --vue-indent-script-and-style --config .prettierrc --write \"src/**/*.{ts,js,vue}\" \"test/**/*.{ts,js}\" \"utils/**/*.{ts,js}\"",
    "prettier-check": "prettier --check --vue-indent-script-and-style --config .prettierrc \"src/**/*.{ts,js,vue}\" \"test/**/*.{ts,js}\" \"utils/**/*.{ts,js}\""
  },
  "dependencies": {
    "tslib": "^2.5.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.31.1",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.0",
    "jsdom": "^22.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "url": "^0.11.0",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.4.2",
    "xml-js": "^1.6.11"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "component",
    "flags",
    "icon",
    "country",
    "countries"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luizzappa/vue-circle-flags"
  },
  "bugs": {
    "url": "https://github.com/luizzappa/vue-circle-flags/issues"
  },
  "author": "Luiz Zappa (luizzappa@hotmail.com)",
  "license": "MIT"
}
